<form id="frmCreate" name="frmCreate" class="form-horizontal" role="form" method="POST">
    <div class="form-group">
        <label class="col-sm-2 control-label no-padding-right">Nome</label>
        <div class="col-sm-10">
            <input id="nome" name="nome" type="text" class="form-control" style="margin-top: 5px;" data-bind="value: nome" maxlength="50" />
        </div>
    </div>
    <div class="space-4"></div>
    <div class="form-group">
        <label class="col-sm-2 control-label no-padding-right" for="Nome">Gerência superior</label>
        <div class="col-sm-10">
            <input type="text" id="createGerenciaSuperiorId" name="createGerenciaSuperiorId" class="form-control autocomplete" style="margin-top: 5px;" />
            <p class="help-block">Informe os 3 primeiros caracteres referentes ao nome da gerência.</p>
        </div>
    </div>
    <div class="space-4"></div>
    @Html.Partial("_Setores", null)
</form>
